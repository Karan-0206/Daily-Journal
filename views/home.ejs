<%- include("partials/header")-%>

        <h1>Home</h1>
        <p><%= start %></p>
        <% home.forEach(function(homie) {%>
        <h1><%= homie["title"] %></h1>
        <% if(homie["content"].length>100) { %>
        <p><%= homie["content"].substring(0,100) + " ..." %><a href="/posts/:<%= homie["_id"] %>">Read More</a></p>
        <% } %> 
        <% if(homie["content"].length<100){ %> 
                <p><%=homie["content"] %></p>
        <% } %>
        <% }) ;%>

<%- include("partials/footer")-%>